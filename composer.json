{
    "name": "yafou/config",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3",
        "symfony/yaml": "^3.4 || ^4.4 || ^5.1"
    },
    "autoload": {
        "psr-4": {
            "YaFou\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YaFou\\Config\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
